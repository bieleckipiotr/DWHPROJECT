          SQLTask:SqlStatementSource="SELECT count(*) &#xA;FROM STOPS s&#xA;LEFT JOIN (&#xA;    SELECT zespol, slupek, MAX(obowiazuje_od) AS latest_obowiazuje_od&#xA;    FROM STOPS&#xA;    GROUP BY zespol, slupek&#xA;    HAVING COUNT(*) &gt; 1&#xA;) subquery ON s.zespol = subquery.zespol AND s.slupek = subquery.slupek AND s.obowiazuje_od &lt; subquery.latest_obowiazuje_od&#xA;WHERE subquery.zespol IS NULL;"
